{
    "name": "magentix/sylius-pickup-demo-plugin",
    "type": "sylius-plugin",
    "description": "Demo Pickup Shipping plugin for Sylius.",
    "license": "MIT",
    "version": "1.3.0",
    "require": {
        "php": "^7.2",
        "sylius/sylius": "~1.3",
        "symfony/symfony": "^3.4|^4.1",
        "magentix/sylius-pickup-plugin": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.0",
        "phpunit/phpunit": "^6.5",
        "sylius-labs/coding-standard": "^2.0"
    },
    "prefer-stable": true,
    "minimum-stability": "alpha",
    "autoload": {
        "psr-4": {
            "Magentix\\SyliusPickupDemoPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/app/AppKernel.php"]
    }
}
